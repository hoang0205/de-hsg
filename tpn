#include <bits/stdc++.h>

using namespace std;
long long n,t;
int main()
{
    freopen("tpn.inp","r",stdin);
    freopen("tpn.out","w",stdout);
    cin>>n;
    int a=cbrt(n);
    for(int i=1;i<a;i++)
    {
        t=t+i*(pow((i+1)*1.0,3)-pow((i)*1.0,3));
    }
    t=t+(n-pow(a*1.0,3)+1)*a;
    cout<<t;
    return 0;
}
